/ humon_node_goal.hbs

= partial "humon_node_header"
/ Sysys.IdxEditableField valueBinding="view.nodeContent.nodeIdx" rawValueBinding="view.nodeContent.nodeIdx"

= each nodeChild in view.nodeContent.nodeVal.requiredChildren
  Sysys.HumonNodeView nodeContentBinding="nodeChild" layoutName="humon_node_fixed_key_layout"

= each nodeChild in view.nodeContent.nodeVal.optionalChildren
  Sysys.HumonNodeView nodeContentBinding="nodeChild"
hr
| Zug test
| view id :
= view._id
ul
  li | view.nodeContent.nodeVal.zug:
  li = view.nodeContent.nodeVal.zug
  li | view.nodeContent.nodeVal.testMe:
  li = view.nodeContent.nodeVal.testMe

hr

span.node-text-left
  / Sysys.HumonNodeView nodeContentBinding="view.nodeContent.$goal" layoutName="humon_node_fixed_key_layout"
ul
  li | view.nodeContent.nodeVal.0
  li = view.nodeContent.nodeVal.0

  li
  li | view.nodeContent.nodeVal.0._value
  li = view.nodeContent.nodeVal.0._value

  li
  li | view.nodeContent.nodeVal.completed_at
  li = view.nodeContent.nodeVal.completed_at

  li
  li | view.nodeContent.nodeVal.$completed_at
  li = view.nodeContent.nodeVal.$completed_at

  li
  li | view.nodeContent.nodeVal.goal
  li = view.nodeContent.nodeVal.goal

  li
  li | view.nodeContent.nodeVal.$goal
  li = view.nodeContent.nodeVal.$goal



  = if view.nodeContent.nodeVal.done
    | Done! Completed at
    = view.nodeContent.nodeVal.completedTimestamp
= partial "humon_node_debug"
